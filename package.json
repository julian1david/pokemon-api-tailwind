{
  "name": "pokemon-tailwind-api",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "prettier --check \"src/**/*.js\"",
    "pre-commit": "pretty-quick --staged",
    "install:husky": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "devDependencies": {
    "gh-pages": "^4.0.0",
    "husky": "7",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "tailwindcss": "^2.0.2",
    "vite": "^3.0.7"
  },
  "dependencies": {
    "autoprefixer": "^10.0.2",
    "postcss": "^8.0.9"
  }
}
